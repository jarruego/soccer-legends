{
  "expo": {
    "name": "Soccer Legends",
    "slug": "soccer-legends",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.soccerlegends.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.soccerlegends",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "orientationLock": "portrait"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "515e826c-c184-4cb8-bbb9-00df226b9d38"
      }
    }
  }
}
